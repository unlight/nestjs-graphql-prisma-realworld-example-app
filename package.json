{
  "name": "nestjs-graphql-prisma-realworld-example-app",
  "private": true,
  "version": "0.0.0-dev",
  "description": "Example real world application built with NestJS, GraphQL and Prisma",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/unlight/nestjs-graphql-prisma-realworld-example-app.git"
  },
  "bugs": {
    "url": "https://github.com/unlight/nestjs-graphql-prisma-realworld-example-app/issues"
  },
  "homepage": "https://github.com/unlight/nestjs-graphql-prisma-realworld-example-app#readme",
  "scripts": {
    "api:spec": "sh Taskfile api_spec",
    "dev": "run-p start:dev prisma:w",
    "preinstall": "sh Taskfile preinstall",
    "postinstall": "patch-package",
    "build": "sh Taskfile build",
    "postbuild": "sh Taskfile postbuild",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "node dist/src/main.js",
    "nest:start": "nest start --path tsconfig.json",
    "start:dev": "node -r dotenv-flow/config node_modules/@nestjs/cli/bin/nest.js start --watch --path tsconfig.json",
    "start:dev:d": "nest start --watch --path tsconfig.json --exec \"node --inspect -r dotenv-flow/config\"",
    "start:debug": "nest start --debug --watch --path tsconfig.json",
    "start:prod": "node dist/src/main",
    "eslint": "eslint \"{src,test}/**/*.ts\"",
    "eslint:fix": "npm run eslint -- --fix",
    "eslint:w": "watchexec -w src \"npm run eslint\"",
    "tslint:fix": "node node_modules/tslint/bin/tslint -p tsconfig.json --fix",
    "lint:fix": "npm run tslint:fix || true && npm run eslint:fix",
    "tscheck": "echo tscheck... && tsc --noEmit --incremental false",
    "tscheck:w": "npm run tscheck -- --watch",
    "test": "npm run eslint && npm run tscheck && npm run test:cov",
    "test:r": "jest --runInBand",
    "test:w": "npm run test:r -- --watch",
    "test:cov": "npm run test:r -- --coverage --verbose",
    "test:d": "node --inspect-brk node_modules/jest/bin/jest --runInBand",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "node node_modules/jest/bin/jest --runInBand --config ./test/jest-e2e.config.js",
    "test:e2e:w": "npm run test:e2e -- --watch",
    "test:api:w": "run-p start:dev test:e2e:w",
    "migrate:save": "node -r dotenv-flow/config node_modules/@prisma/cli/build/index.js migrate save --experimental",
    "migrate:save:preview": "npm run migrate:save -- --preview",
    "migrate:up": "node -r dotenv-flow/config node_modules/@prisma/cli/build/index.js migrate up --experimental --verbose",
    "migrate:up:preview": "npm run migrate:up -- --preview",
    "migrate:down": "node -r dotenv-flow/config node_modules/@prisma/cli/build/index.js migrate down --experimental 1",
    "prisma:w": "node -r dotenv-flow/config node_modules/@prisma/cli/build/index.js generate --watch",
    "prisma:g": "node -r dotenv-flow/config node_modules/@prisma/cli/build/index.js generate",
    "prisma:seed": "node -r dotenv-flow/config -r ts-node/register prisma/seed",
    "prisma:studio": "node -r dotenv-flow/config node_modules/@prisma/cli/build/index.js studio --experimental",
    "dev:update": "updtr --out --to latest --test \"npm test:r\"",
    "sync-dotenv": "npx sync-dotenv"
  },
  "dependencies": {
    "@nestjs/common": "^6.11.11",
    "@nestjs/config": "^0.4.0",
    "@nestjs/core": "^6.11.11",
    "@nestjs/graphql": "^6.6.2",
    "@nestjs/jwt": "^6.1.2",
    "@nestjs/passport": "^6.2.0",
    "@nestjs/platform-express": "^6.11.11",
    "@prisma/client": "^2.0.0-beta.3",
    "@sindresorhus/slugify": "^0.11.0",
    "apollo-server-express": "^2.12.0",
    "class-transformer": "^0.2.3",
    "class-validator": "^0.12.1",
    "graphql": "^14.6.0",
    "graphql-fields": "^2.0.3",
    "graphql-info-transformer": "0.0.1",
    "graphql-request": "^1.8.2",
    "graphql-tools": "^4.0.7",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^6.5.5",
    "tslib": "^1.11.1",
    "type-graphql": "^0.17.6"
  },
  "husky": {
    "hooks": {
      "pre-commit": "precise-commits",
      "pre-push": "npm run test"
    }
  },
  "devDependencies": {
    "@nestjs/cli": "^6.14.2",
    "@nestjs/schematics": "^6.9.4",
    "@nestjs/testing": "^6.11.11",
    "@prisma/cli": "^2.0.0-beta.3",
    "@types/express": "^4.17.0",
    "@types/graphql-fields": "^1.3.2",
    "@types/jest": "^25.2.1",
    "@types/node": "^13.13.2",
    "@types/passport-jwt": "^3.0.3",
    "@types/supertest": "^2.0.8",
    "@types/wait-on": "^4.0.0",
    "@typescript-eslint/eslint-plugin": "^2.29.0",
    "@typescript-eslint/eslint-plugin-tslint": "^2.29.0",
    "@typescript-eslint/parser": "^2.29.0",
    "common-tags": "^1.8.0",
    "dotenv-flow": "^3.1.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-import-resolver-node": "^0.3.3",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jest": "^23.8.2",
    "eslint-plugin-only-warn": "^1.0.2",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-simple-import-sort": "^5.0.2",
    "eslint-plugin-sonarjs": "^0.5.0",
    "eslint-plugin-unicorn": "^18.0.1",
    "eslint-plugin-wix-editor": "^3.0.0",
    "git-branch-is": "^3.1.0",
    "husky": "^4.2.5",
    "jest": "^25.4.0",
    "newman": "^5.0.0",
    "node-fetch": "^2.6.0",
    "npm-run-all": "^4.1.5",
    "patch-package": "^6.2.2",
    "precise-commits": "^1.0.2",
    "prettier": "^2.0.5",
    "simplytyped": "^3.2.3",
    "supertest": "^4.0.2",
    "to-mock": "^1.6.2",
    "ts-jest": "^25.4.0",
    "ts-loader": "^7.0.1",
    "ts-node": "^8.9.0",
    "tsconfig-paths": "^3.9.0",
    "tslint": "^6.1.1",
    "tslint-clean-code": "^0.2.10",
    "tslint-consistent-codestyle": "^1.16.0",
    "tslint-etc": "^1.10.1",
    "tslint-microsoft-contrib": "^6.2.0",
    "tslint-sonarts": "^1.9.0",
    "typegraphql-prisma": "^0.2.1",
    "typescript": "^3.8.3",
    "wait-on": "^4.0.2",
    "watchexec-bin": "^1.0.0"
  }
}
