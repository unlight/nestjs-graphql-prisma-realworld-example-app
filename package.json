{
  "name": "nestjs-graphql-prisma-realworld-example-app",
  "private": true,
  "version": "0.0.0-dev",
  "description": "Example real world application built with NestJS, GraphQL and Prisma",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/unlight/nestjs-graphql-prisma-realworld-example-app.git"
  },
  "bugs": {
    "url": "https://github.com/unlight/nestjs-graphql-prisma-realworld-example-app/issues"
  },
  "homepage": "https://github.com/unlight/nestjs-graphql-prisma-realworld-example-app#readme",
  "scripts": {
    "api:spec": "sh Taskfile api_spec",
    "dev": "run-p start:dev prisma:w",
    "preinstall": "sh Taskfile preinstall",
    "postinstall": "patch-package",
    "build": "sh Taskfile build",
    "postbuild": "sh Taskfile postbuild",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "node dist/src/main.js",
    "nest:start": "nest start --path tsconfig.json",
    "start:dev": "node -r dotenv-flow/config node_modules/@nestjs/cli/bin/nest.js start --path tsconfig.json --watch --exec \"node -r source-map-support/register\"",
    "start:dev:1": "node node_modules/webpack/bin/webpack.js --watch",
    "start:dev:2": "node node_modules/webpack/bin/webpack.js --watch --env hmr2",
    "start:dev:d": "nest start --watch --path tsconfig.json --exec \"node --inspect -r dotenv-flow/config\"",
    "start:debug": "nest start --debug --watch --path tsconfig.json",
    "start:prod": "node dist/src/main",
    "eslint": "eslint \"{src,test}/**/*.ts\"",
    "eslint:fix": "npm run eslint -- --fix",
    "eslint:w": "watchexec -w src \"npm run eslint\"",
    "tscheck": "echo tscheck... && tsc --noEmit --incremental false",
    "tscheck:w": "npm run tscheck -- --watch",
    "test": "npm run eslint && npm run tscheck && npm run test:cov",
    "test:r": "jest --runInBand",
    "test:w": "npm run test:r -- --watch",
    "test:cov": "jest --coverage --verbose",
    "test:d": "node --inspect-brk node_modules/jest/bin/jest --runInBand",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "node node_modules/jest/bin/jest --runInBand --config ./test/jest-e2e.config.js",
    "test:e2e:w": "npm run test:e2e -- --watch",
    "test:api:w": "run-p start:dev test:e2e:w",
    "migrate:save": "node -r dotenv-flow/config node_modules/@prisma/cli/build/index.js migrate save --experimental",
    "migrate:save:preview": "npm run migrate:save -- --preview",
    "migrate:up": "node -r dotenv-flow/config node_modules/@prisma/cli/build/index.js migrate up --experimental --verbose",
    "migrate:up:preview": "npm run migrate:up -- --preview",
    "migrate:down": "node -r dotenv-flow/config node_modules/@prisma/cli/build/index.js migrate down --experimental 1",
    "prisma:w": "node -r dotenv-flow/config node_modules/@prisma/cli/build/index.js generate --watch",
    "prisma:g": "node -r dotenv-flow/config node_modules/@prisma/cli/build/index.js generate",
    "regen": "rm -rf src/@generated && npm run prisma:g && npm run eslint:fix",
    "prisma:seed": "node -r dotenv-flow/config -r ts-node/register prisma/seed",
    "prisma:studio": "node -r dotenv-flow/config node_modules/@prisma/cli/build/index.js studio --experimental",
    "dev:update": "updtr --out --to latest --test \"npm test:r\"",
    "sync-dotenv": "npx sync-dotenv"
  },
  "dependencies": {
    "@nestjs-steroids/environment": "^1.0.2",
    "@nestjs/common": "^7.6.1",
    "@nestjs/core": "^7.6.1",
    "@nestjs/graphql": "^7.9.1",
    "@nestjs/jwt": "^7.2.0",
    "@nestjs/passport": "^7.1.5",
    "@nestjs/platform-express": "^7.6.1",
    "@paljs/plugins": "^2.9.0",
    "@prisma/client": "^2.13.0",
    "@sindresorhus/slugify": "^1.1.0",
    "apollo-error-converter": "^1.1.1",
    "apollo-server-express": "^2.19.0",
    "class-transformer": "^0.3.1",
    "class-validator": "^0.12.2",
    "dotenv-flow": "^3.2.0",
    "graphql": "^15.4.0",
    "graphql-fields": "^2.0.3",
    "graphql-request": "^3.3.0",
    "jsonwebtoken": "^8.5.1",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^6.6.3",
    "tslib": "^2.0.3"
  },
  "devDependencies": {
    "@nestjs/cli": "^7.5.4",
    "@nestjs/schematics": "^7.2.5",
    "@nestjs/testing": "^7.6.1",
    "@prisma/cli": "^2.13.0",
    "@types/express": "^4.17.9",
    "@types/graphql-fields": "^1.3.3",
    "@types/jest": "^26.0.19",
    "@types/node": "^14.14.13",
    "@types/passport-jwt": "^3.0.3",
    "@types/supertest": "^2.0.10",
    "@types/wait-on": "^5.2.0",
    "@types/webpack-env": "^1.16.0",
    "@typescript-eslint/eslint-plugin": "^4.9.1",
    "@typescript-eslint/parser": "^4.9.1",
    "common-tags": "^1.8.0",
    "eslint": "^7.15.0",
    "eslint-config-prettier": "^7.0.0",
    "eslint-import-resolver-node": "^0.3.4",
    "eslint-plugin-boundaries": "^1.1.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.1.3",
    "eslint-plugin-only-warn": "^1.0.2",
    "eslint-plugin-prettier": "^3.2.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "eslint-plugin-sonarjs": "^0.5.0",
    "eslint-plugin-unicorn": "^23.0.0",
    "eslint-plugin-wix-editor": "^3.2.0",
    "git-branch-is": "^4.0.0",
    "husky": "^5.0.4",
    "jest": "^26.6.3",
    "jest-createspyobj": "^2.0.0",
    "log-process-errors": "^6.2.0",
    "newman": "^5.2.1",
    "node-fetch": "^2.6.1",
    "node-hot-loader": "^1.21.0",
    "npm-run-all": "^4.1.5",
    "patch-package": "^6.2.2",
    "precise-commits": "^1.0.2",
    "prettier": "^2.2.1",
    "prisma-nestjs-graphql": "^6.0.0",
    "simplytyped": "^3.3.0",
    "start-server-nestjs-webpack-plugin": "^2.2.5",
    "supertest": "^6.0.1",
    "ts-jest": "^26.4.4",
    "ts-loader": "^8.0.12",
    "ts-mockito": "^2.6.1",
    "ts-node": "^9.1.1",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.1.3",
    "universal-mock": "^1.1.0",
    "wait-on": "^5.2.0",
    "watchexec-bin": "^1.0.0",
    "webpack": "^5.10.1",
    "webpack-cli": "^4.2.0",
    "webpack-node-externals": "^2.5.2"
  }
}
